        {
            "fileName" : "document.txt",
            "targetName" : "$itemname$.html",
            "replaceParameters" : true,
            "open" : true
        },
        {
            "fileName" : "css.txt",
            "targetName" : "$itemname$.css",
            "replaceParameters" : true,
            "open" : true
        }        
    ]
